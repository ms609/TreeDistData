              title = "Untitled",
              author = personList(as.person("Martin R. Smith")),
              journal = "Unsubmitted",
              year = "20XX",
     
              textVersion =
              paste0("Smith, M.R. (20XX).",
                     " Untitled", 
                     " Journal name.",
